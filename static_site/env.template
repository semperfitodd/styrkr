# Styrkr Frontend Environment Variables
# Copy this file to .env.local and fill in your values
# Get these values from: cd ../terraform && terraform output

# API Gateway URL (use custom domain with https://, not execute-api URL)
# Example: https://dev-api.yourdomain.com
REACT_APP_API_URL=https://YOUR_API_DOMAIN

# Cognito Configuration (domain only, no https://)
# Example: dev-auth.yourdomain.com
REACT_APP_COGNITO_DOMAIN=YOUR_AUTH_DOMAIN

# Cognito Client ID (from terraform output cognito_client_id)
REACT_APP_COGNITO_CLIENT_ID=YOUR_CLIENT_ID

# Frontend URL for OAuth callbacks (with https://)
# Example: https://dev.yourdomain.com
REACT_APP_REDIRECT_URI=https://YOUR_FRONTEND_DOMAIN/auth/callback
REACT_APP_LOGOUT_URI=https://YOUR_FRONTEND_DOMAIN/logout

